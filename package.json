{
  "name": "airtable-ofetch",
  "version": "0.1.4",
  "description": "JS Client for Airtable Web API built on top @unjs/ofetch",
  "repository": "nadhifikbarw/airtable-ofetch",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "dev": "vitest",
    "build": "unbuild",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . && prettier -c src tests",
    "lint:fix": "eslint --fix . && prettier -w src tests",
    "release": "pnpm test && changelogen --release && npm publish && git push --follow-tags",
    "test": "pnpm lint && vitest run --coverage",
    "prepack": "pnpm build"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "defu": "^6.1.4",
    "ofetch": "^1.4.1"
  },
  "devDependencies": {
    "@types/node": "^22.18.12",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/eslint-plugin": "^1.3.26",
    "changelogen": "^0.6.2",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-config-unjs": "^0.5.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "unbuild": "^3.6.1",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.20.0"
}
